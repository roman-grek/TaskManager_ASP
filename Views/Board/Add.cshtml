@model Board
@{
    ViewData["Title"] = "Add Board";
}
<div class=container>
    <form method="post">
        <label>Title of Board:</label>
        <input asp-for="Title" placeholder="Title">
        <input hidden asp-for="UserId" Model.UserId>
        <button class="btn center-align" type="submit">
            Create <i class="material-icons right">send</i>
        </button>
    </form>
</div>